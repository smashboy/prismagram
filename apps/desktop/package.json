{
  "name": "prismagram",
  "version": "0.1.2",
  "description": "Visual prisma schema builder",
  "main": "./out/main/index.js",
  "author": "https://github.com/smashboy",
  "homepage": "https://github.com/smashboy/prismagram",
  "repository": "https://github.com/smashboy/prismagram.git",
  "license": "MIT",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "electron-vite build && electron-builder --mac --config",
    "build:linux": "electron-vite build && electron-builder --linux --config"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.0.7",
    "@dnd-kit/modifiers": "^6.0.1",
    "@dnd-kit/sortable": "^7.0.2",
    "@electron-toolkit/preload": "^1.0.2",
    "@electron-toolkit/utils": "^1.0.2",
    "@emotion/react": "^11.10.5",
    "@mantine/core": "5.10.5",
    "@mantine/hooks": "5.10.5",
    "@mantine/notifications": "5.10.5",
    "@mantine/prism": "5.10.5",
    "@mantine/spotlight": "5.10.5",
    "@mantine/tiptap": "5.10.5",
    "@monaco-editor/react": "^4.4.6",
    "@prisma/internals": "^4.7.1",
    "@prisma/prisma-fmt-wasm": "^4.8.0-44.574e45f3fd7c55b8ffcea000812329a79e7f2a22",
    "@prisma/text-editors": "^0.0.22",
    "@shared/common": "*",
    "@tabler/icons": "^1.115.0",
    "@tiptap/extension-code-block-lowlight": "^2.0.0-beta.207",
    "@tiptap/extension-link": "^2.0.0-beta.207",
    "@tiptap/react": "^2.0.0-beta.207",
    "@tiptap/starter-kit": "^2.0.0-beta.207",
    "cmdk": "^0.1.22",
    "color-alpha": "^1.1.3",
    "dagre": "^0.8.5",
    "effector": "^22.4.0",
    "effector-react": "^22.3.4",
    "electron-updater": "^5.3.0",
    "kill-port-process": "^3.1.0",
    "lodash": "^4.17.21",
    "lowlight": "^2.8.0",
    "monaco-editor": "^0.34.1",
    "patronum": "^1.12.3",
    "prisma-state": "*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-use": "^17.4.0",
    "reactflow": "^11.5.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^1.2.3",
    "@types/lodash": "^4.14.191",
    "@types/node": "16.11.22",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "@vitejs/plugin-react": "^2.2.0",
    "electron": "21.3.0",
    "electron-builder": "^23.6.0",
    "electron-vite": "^1.0.14",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "prettier": "^2.7.1",
    "typescript": "^4.9.3",
    "vite": "^3.2.4",
    "vite-plugin-monaco-editor": "^1.1.0"
  }
}
